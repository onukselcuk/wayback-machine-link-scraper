{
	"name": "wayback-machine-link-scraper",
	"version": "1.0.0",
	"description": "wayback machine link scraper app",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"electron:watch": "nodemon --watch * --exec 'electron .'",
		"package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
		"package-win": "electron-packager . wayback-machine-link-scraper --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Shopping List\"",
		"package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
	},
	"author": "Selcuk",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.19.0",
		"electron": "^6.0.10",
		"electron-reload": "^1.5.0",
		"jquery": "^3.4.1",
		"moment": "^2.24.0"
	},
	"devDependencies": {
		"electron-packager": "^14.0.6"
	}
}
